# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id
REACT_APP_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Xero Configuration
# You need to create a new app in the Xero Developer Portal and update these credentials
REACT_APP_XERO_CLIENT_ID=your-xero-client-id
REACT_APP_XERO_CLIENT_SECRET=your-xero-client-secret
# Xero Webhook Key - Get this from Xero Developer Portal after setting up webhooks
REACT_APP_XERO_WEBHOOK_KEY=your-xero-webhook-key

# PayPal Configuration
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id
REACT_APP_PAYPAL_SECRET=your-paypal-client-secret

# Stripe Configuration
REACT_APP_STRIPE_CLIENT_ID=your-stripe-client-id
REACT_APP_STRIPE_SECRET_KEY=your-stripe-secret-key

# Plaid Configuration
PLAID_CLIENT_ID=your-plaid-client-id
PLAID_SECRET=your-plaid-secret
PLAID_ENV=sandbox
PLAID_REDIRECT_URI=http://localhost:3000/plaid/oauth-return

# Application URL (for OAuth redirects)
REACT_APP_URL=http://localhost:3000